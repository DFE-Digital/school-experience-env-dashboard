<p id="notice"><%= notice %></p>

<h1>Environments</h1>

<table>
  <thead>
    <tr>
      <th align="left">Name</th>
      <th align="left">Version</th>
<!--  <th>Git hub release</th>
      <th>Db url</th>
      <th>Db user</th>-->
    </tr>
  </thead>

  <tbody>
    <% @environments.each do |environment| %>
      <tr>
        <td align="left"><a href="<%= environment.url %>"><%= environment.name %></a></td>
        <td align="left"><%= environment.version %> (<a href="<%= environment.git_hub_release %>">release</a>)</td> 
<!--        <td><%= environment.git_hub_release %></td>
        <td><%= environment.db_url %></td>
        <td><%= environment.db_user %></td>-->
      </tr>
    <% end %>
  </tbody>
</table>

